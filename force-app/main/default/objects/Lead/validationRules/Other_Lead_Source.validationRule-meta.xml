<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Lead_Source</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL( LeadSource , &quot;Other&quot;),
ISBLANK( Other_Lead_Source__c ))</errorConditionFormula>
    <errorDisplayField>Other_Lead_Source__c</errorDisplayField>
    <errorMessage>Please provide &apos;Other&apos; lead source</errorMessage>
</ValidationRule>
